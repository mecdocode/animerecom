{
  "version": 2,
  "public": true,
  "buildCommand": "CI=false npm run build",
  "functions": {
    "api/recommendations.js": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/recommendations.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/recommendations",
      "dest": "/api/recommendations"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
